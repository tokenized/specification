version 0

OutpointField {
  1 TxId        binary
  2 OutputIndex uint32
}

InitiateRelationship {
  1 Type                uint8
  2 Seed                binary
  3 Flag                binary
  4 EncryptionType      uint8
  5 ProofOfIdentityType uint8
  6 ProofOfIdentity     binary
  7 ChannelParties      []*ChannelPartyField
}

ChannelPartyField {
  1 AdministrativeAddress binary
  2 OutputIndex           uint32
}

PublicMessage {
  1 Timestamp     uint64
  2 Subject       string
  3 Regarding     *OutpointField
  4 PublicMessage *DocumentField
  5 Attachments   []*DocumentField
}

SignatureRequest {
  1 Timestamp uint64
  2 Payload   binary
}

Distribution {
  1 InstrumentCode binary
  2 Timestamp      uint64
}

AcceptRelationship {
  1 ProofOfIdentityType uint8
  2 ProofOfIdentity     binary
}

DocumentField {
  1 Name     string
  2 Type     string
  3 Contents binary
}

PrivateMessage {
  1 Timestamp      uint64
  2 Subject        string
  3 Regarding      *OutpointField
  4 PrivateMessage *DocumentField
  5 Attachments    []*DocumentField
}

Offer {
  1 Timestamp uint64
  2 Payload   binary
}

SettlementRequest {
  1 Timestamp    uint64
  2 TransferTxId binary
  3 ContractFees []*TargetAddressField
  4 Settlement   binary
}

TargetAddressField {
  1 Address  binary
  2 Quantity uint64
}

PendingAcceptRelationship {
  1 ProofOfIdentityType uint8
  2 ProofOfIdentity     binary
}

RevertedTx {
  1 Timestamp   uint64
  2 Transaction binary
}

OutputMetadata {
  1 OutputDescription string
  2 Tags              binary
  3 CustomTags        []*OutputTagField
}

OutputTagField {
  1 Tag string
}

RelationshipAmendment {
  1 Seed                 binary
  2 BaseEncryptionSecret binary
  3 AddMemberIndexes     uint32
  4 DropMemberIndexes    uint32
}

InitiateThread {
  1 Flag binary
  2 Seed binary
}


